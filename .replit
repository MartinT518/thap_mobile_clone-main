modules = ["ruby-3.2", "swift-5.8", "dart-3.8"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["flutter"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Flutter Web App"

[[workflows.workflow]]
name = "Flutter Web App"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "export PATH=\"$PATH:$HOME/.pub-cache/bin\" && cd source/build/web && dhttpd --host=0.0.0.0 --port=5000"
waitForPort = 5000

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 36379
externalPort = 3001
